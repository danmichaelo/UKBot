msgid "max"
msgstr "maks"

#: ukrules.py:53
msgid "new page"
msgstr "uusi sivu"

#: ukrules.py:64
msgid "redirect"
msgstr "uudelleenohjaus"

#: ukrules.py:138
#, python-format
msgid "removal of {{tl|%(template)s}}"
